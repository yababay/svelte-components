{
	"name": "@yababay67/svelte-components",
	"version": "1.0.8",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"push": "npm publish --access public",
		"checkout:template": "git add . && git commit -am ok && git checkout template",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./markdown": "./dist/server/markdown.js",
		"./icons": "./dist/server/icons.js",
		"./types": "./dist/types.d.ts",
		"./layout/dummy.scss": "./dist/styles/layouts/dummy.scss",
		"./layout/publications.scss": "./dist/styles/layouts/publications/index.scss"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^1.20.4",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/package": "^2.0.0",
		"@types/showdown": "^2.0.4",
		"@yababay67/styles": "^0.0.1",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"bootstrap-icons": "^1.11.1",
		"showdown": "^2.1.0"
	}
}
